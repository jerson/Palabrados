{
  "name": "Palabrados",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.js",
  "devDependencies": {
    "electron": "^1.4.12",
    "electron-packager": "^8.4.0",
    "react-scripts": "0.8.4"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "soundmanager2": "^2.97.20150601-a"
  },
  "scripts": {
    "electron": "electron ./build",
    "build:app:mac": "npm run build && electron-packager ./build Palabrados --out=releases --icon=build/icon.icns --platform=darwin --overwrite",
    "build:app:windows": "npm run build && electron-packager ./build Palabrados --out=releases --icon=build/favicon.ico --platform=win32 --overwrite",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
